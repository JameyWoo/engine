    <span class="hljs-keyword">for</span> (<span class="hljs-keyword">int</span> i = <span class="hljs-number">1</span>; i &lt;= n; ++i) {
        <span class="hljs-built_in">cin</span> &gt;&gt; orig[i];
        srt[i] = orig[i];
    }
    sort(srt + <span class="hljs-number">1</span>, srt + n + <span class="hljs-number">1</span>);
    <span class="hljs-built_in">cin</span> &gt;&gt; p;
    <span class="hljs-keyword">char</span> ans[<span class="hljs-number">10005</span>];
    <span class="hljs-keyword">char</span> c = orig[p];
    <span class="hljs-keyword">bool</span> vis[<span class="hljs-number">10005</span>] = {};
    <span class="hljs-keyword">for</span> (<span class="hljs-keyword">int</span> i = <span class="hljs-number">1</span>; i &lt;= n; ++i) {
        <span class="hljs-keyword">if</span> (c == srt[i]) {
            c = orig[i];
            vis[i] = <span class="hljs-keyword">true</span>;
            <span class="hljs-keyword">break</span>;
        }
    }
    <span class="hljs-keyword">int</span> cnt = <span class="hljs-number">0</span>;
